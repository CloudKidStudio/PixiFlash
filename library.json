{
	"name": "Pixi Flash",
	"version": "0.2.5",
	"description": "Runtime library to play HTML5 Canvas content from Flash with Pixi.js",
	"url": "https://github.com/CloudKidStudio/PixiFlash",
	"output": "pixi-flash",
	"main": [
		"src/data/BISON.js",
		"src/data/BISONLoader.js",
		"src/flash/main.js",
		"src/mixins/*.js"
	],
	"modules":
	{
		"springroll": [
			"src/plugins/springroll/FlashArt.js",
			"src/plugins/springroll/FlashArtTask.js",
			"src/plugins/springroll/MovieClipInstance.js",
			"src/plugins/springroll/PixiFlashPlugin.js",
			"src/plugins/springroll/SpriteSheetTask.js"
		]
	}
}